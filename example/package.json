{
  "name": "onnxruntime-react-native-jsi-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android": "react-native build-android --extra-params \"--no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a\"",
    "build:ios": "react-native build-ios --scheme OnnxruntimeReactNativeJsiExample --mode Debug --extra-params \"-sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO\""
  },
  "dependencies": {
    "@dr.pogodin/react-native-fs": "^2.34.0",
    "@flyerhq/react-native-chat-ui": "^1.4.3",
    "@huggingface/transformers": "npm:@fugood/transformers@3.7.1-beta.1",
    "@petamoriken/float16": "^3.9.2",
    "@react-native-documents/picker": "^10.1.5",
    "@react-native/new-app-screen": "0.80.2",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.24",
    "react": "19.1.0",
    "react-native": "0.80.2",
    "react-native-audio-api": "^0.7.0",
    "react-native-device-info": "^14.0.4",
    "react-native-safe-area-context": "^5.6.0",
    "react-native-screens": "^4.13.1",
    "text-encoding-polyfill": "^0.6.7",
    "xregexp": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.1.1",
    "@react-native-community/cli-platform-android": "19.1.1",
    "@react-native-community/cli-platform-ios": "19.1.1",
    "@react-native/babel-preset": "0.80.2",
    "@react-native/metro-config": "0.80.2",
    "@react-native/typescript-config": "0.80.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "react-native-builder-bob": "^0.40.13"
  },
  "engines": {
    "node": ">=18"
  }
}
